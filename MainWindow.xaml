<Window x:Class="onlineData.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:onlineData"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" FontSize="24">
    <StackPanel>
        <Label>Key</Label>
        <TextBox x:Name="txtKey"/>
        <Label>Value</Label>
        <TextBox x:Name="txtValue"/>
        <Button x:Name="btnReadFromBuddy" Background="LightBlue" Content="Read from Buddy" Click="btnReadFromBuddy_Click" />
        <Button x:Name="btnWriteToBuddy" Background="LightGreen" Content="Write to Buddy" Click="btnWriteToBuddy_Click"/>

    </StackPanel>
</Window>
